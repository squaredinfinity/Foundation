    <Copyright>Copyright © Squared Infinity</Copyright>
    <Tags>
    </Tags>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Graphics.ColorSpaces\Graphics.ColorSpaces.csproj" />
  </ItemGroup>
  <Import Project="$(NuProjPath)\NuProj.targets" />
</Project>